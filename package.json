    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "prettier": "3.1.1",
    "storybook": "^7.6.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^1.0.4"
